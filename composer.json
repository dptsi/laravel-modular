  "description": "Laravel modular template developed for DPTSI projects",
  "type": "library",

  "authors": [
    {
      "name": "DPTSI ITS",
      "email": "developer@its.ac.id"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4|8.0.*",
